@model List<employee>
@{
    ViewData["Title"] = "index";
}

<h1>index</h1>

<table class="table table-bordered table-info">
   <tr>

        <th>ID</th>
        <th>Name</th>
        <th>salary</th>
        <th>jobtitle</th>
        <th>Address</th>
   </tr>
  @foreach(var i in Model)
    {
        <tr>
            <th>@i.id</th>
            <th>@i.name</th>
            <th>@i.salary</th>
            <th>@i.jobtitle</th>
            <th><a href="/employee/edit/@i.id">edit</a></th>

        </tr>
    }



</tabl>